:root {
  --baseColor: var(--baseColor, #000);
  --primaryColor: var(--primaryColor, green);
  --errorColor: var(--errorColor, #b72525);
  --errorBgColor: var(--errorBgColor, #f9e6e6);
  --borderRadius: var(--borderRadius, 2px);
  --secondaryColor: var(--secondaryColor, blue);
  --inputWidth: 250px;
  --inputLineHeight: 1.6em;
  --inputBgColor: #fdfdfd;
  --fieldsetBgColor: #f5f5f5;
  --fieldsetBorder: none;
  --placeholderColor: #6a6a6a;
  --focusColor: Highlight;
  --successColor: lightgreen;
  --borderColor: #ccc;
  --checkedColor: #000;
  --primaryFont: 'Helvetica';
  --spacingUnit: 1em;
  --iconCheckbox: url('data:image/svg+xml;base64,');
  --buttonColor: #333;
  --buttonColorHover: #555;
  --buttonTextColor: #fff;
  --buttonSecondaryColor: #333;
  --buttonSecondaryColorHover: #555;
  --buttonSecondaryTextColor: #fff;
  --buttonColorDisabled: lightgrey;
  --formMaxWidth: 740px;
  --formMargin: 0;
}
